 (\r\n    <Wrapper>\r\n      <Header />\r\n      <MapViewWrapper>\r\n        <MapView onLoad={() => setMapLoaded(true)} />\r\n        {\r\n          mapLoaded ? \r\n          <React.Fragment>\r\n            <Write onClick={() => navigate(\"/write\")}>\r\n              <i className=\"fa fa-bullhorn\"></i>\r\n            </Write>\r\n            <NewsContextProvider>\r\n              <Drawer />\r\n            </NewsContextProvider>\r\n          </React.Fragment> :\r\n          <LoadingMap theme={theme}>Loading map...</LoadingMap>\r\n        }\r\n      </MapViewWrapper>\r\n      <Navbar />\r\n    </Wrapper>\r\n  );\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"6087e389af2b253afdc03a671d130abb","cacheData":{"env":{}}}